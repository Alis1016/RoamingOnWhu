<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SplashActivity"
    android:background="@drawable/openpage">

    <ImageView
        android:id="@+id/iv_0"
        android:layout_width="198dp"
        android:layout_height="184dp"
        android:padding="1dp"
        android:scaleType="fitXY"
        android:src="@drawable/openpage_insert"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="36dp"
        android:layout_marginTop="188dp" />

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="210dp"
        android:layout_height="52dp"
        android:text="武大漫游"
        android:textSize="40sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@id/iv_0"
        app:layout_constraintTop_toBottomOf="@id/iv_0"
        android:layout_marginTop="16dp" />

    <com.example.myapplication01.util.CTextView
        android:id="@+id/ct"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        app:layout_constraintStart_toStartOf="@id/tv_title"
        app:layout_constraintTop_toBottomOf="@id/tv_title"
        android:layout_marginTop="8dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
